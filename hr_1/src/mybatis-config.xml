<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
<typeAliases>
	<!-- 用户表 -->
	<typeAlias type="com.zd.entity.user" alias="user"/>
	<!-- 一级机构实体类 -->
	<typeAlias type="com.zd.entity.Config_file_first_kind" alias="Config_file_first_kind"/>
	<!-- 二级机构 -->
	<typeAlias type="com.zd.entity.Config_file_second_kind" alias="Config_file_second_kind"/>
</typeAliases>

<mappers>
	<!-- 用户表 -->
	<mapper resource="com/zd/mapper/UserMapper.xml"/>
	<!-- 一级机构mappers -->
	<mapper resource="com/zd/mapper/Config_file_first_kindMapper.xml"/>
	<!-- 二级机构 -->
	<mapper resource="com/zd/mapper/Config_file_second_kindMapper.xml"/>
</mappers>
</configuration>
