<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
<typeAliases>
	<!-- 用户表 -->
	<typeAlias type="com.zd.entity.user" alias="user"/>
	<!-- 一级机构实体类 -->
	<typeAlias type="com.zd.entity.Config_file_first_kind" alias="Config_file_first_kind"/>
	<!-- 二级机构 -->
	<typeAlias type="com.zd.entity.Config_file_second_kind" alias="Config_file_second_kind"/>
	<!-- 三级机构 -->
	<typeAlias type="com.zd.entity.Config_file_third_kind" alias="Config_file_third_kind"/>
	<!-- 公共表 -->
	<typeAlias type="com.zd.entity.Config_public_char" alias="Config_public_char"/>
	<!-- 职业分类一级联动 -->
	<typeAlias type="com.zd.entity.Config_major_kind" alias="Config_major_kind"/>
	<!-- 职业分类二级联动 -->
	<typeAlias type="com.zd.entity.Config_major" alias="Config_major"/>
	<!-- 职业发表登记 -->
	<typeAlias type="com.zd.entity.Engage_major_release" alias="Engage_major_release"/>
	<!-- 公共表 -->
	<typeAlias type="com.zd.entity.Config_public_char" alias="Config_public_char"/>
	<!-- 薪资分发 -->
	<typeAlias type="com.zd.entity.Salary_grant" alias="Salary_grant"/>
	
</typeAliases>

<mappers>
	<!-- 用户表 -->
	<mapper resource="com/zd/mapper/UserMapper.xml"/>
	<!-- 一级机构mappers -->
	<mapper resource="com/zd/mapper/Config_file_first_kindMapper.xml"/>
	<!-- 二级机构 -->
	<mapper resource="com/zd/mapper/Config_file_second_kindMapper.xml"/>
	<!-- 三级机构 -->
	<mapper resource="com/zd/mapper/Config_file_third_kindMapper.xml"/>
	<!-- 公共表 -->
	<mapper resource="com/zd/mapper/Config_public_charMapper.xml"/>
	<!-- 职业分类一级联动 -->
	<mapper resource="com/zd/mapper/Config_major_kindMapper.xml"/>
	<!-- 职业分类二级联动 -->
	<mapper resource="com/zd/mapper/Config_majorMapper.xml"/>
	<!-- 职业发表登记 -->
	<mapper resource="com/zd/mapper/Engage_major_releaseMapper.xml"/>
	<!-- 公共表 -->
	<mapper resource="com/zd/mapper/Config_public_charMapper.xml"/>
	<!-- 薪资分发 -->
	<mapper resource="com/zd/mapper/Salary_grantMapper.xml"/>
	<!-- 薪酬标准 -->
	<mapper resource="com/zd/mapper/Salary_StandardMapper.xml"/>
	<!-- zm_some表的映射 -->
	<mapper resource="com/zd/mapper/zm_someMapper.xml"/>
</mappers>
</configuration>
